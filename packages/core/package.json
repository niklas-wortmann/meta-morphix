{
	"name": "@wandel/core",
	"version": "0.0.0",
	"private": true,
	"description": "CLI for Code Transformations and Generations",
	"repository": {
		"type": "git",
		"url": "https://github.com/wandeljs/wandel"
	},
	"license": "MIT",
	"author": {
		"name": "Jan-Niklas Wortmann",
		"email": "jan@wordman.dev"
	},
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		},
		"./types": {
			"types": "./dist/types.d.mts"
		},
		"./dist/*": "./dist/*",
		"./package.json": "./package.json",
		"./*": "./dist/*"
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"typesVersions": {
		"*": {
			"types": [
				"./dist/types.d.mts"
			],
			"*": [
				"./dist/*",
				"./*"
			]
		}
	},
	"files": [
		"dist",
		"LICENSE.md",
		"README.md"
	],
	"scripts": {
		"build": "rimraf dist && rollup -c",
		"dev": "rollup -cw",
		"prepublishOnly": "pnpm build",
		"test": ""
	},
	"publishConfig": {
		"provenance": true
	}
}
